{
  "name": "anime-recommender",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "next": "NODE_NO_WARNINGS=1 next dev",
    "debug": "concurrently \"NEXT_DEBUG_TRACE=1 next dev\" \"pnpm run flask\"",
    "flask": "cd ../server && python app.py",
    "stack": "concurrently \"pnpm run next\" \"pnpm run flask\"",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "prisma-migrate": "pnpm exec prisma migrate dev",
    "prisma-generate": "pnpm exec prisma generate",
    "prisma-seed": "pnpm exec prisma db seed",
    "prisma-update": "pnpm run prisma-migrate && pnpm run prisma-generate",
    "prisma-reset": "pnpm exec prisma db push --force-reset"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@emotion/react": "^11.13.5",
    "@emotion/styled": "^11.13.5",
    "@heroicons/react": "^2.2.0",
    "@heroui/button": "^2.2.15",
    "@heroui/react": "^2.7.4",
    "@prisma/client": "6.0.0",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "eslint": "^8.57.1",
    "eslint-config-next": "13.4.2",
    "framer-motion": "^11.12.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.0.3",
    "next-auth": "5.0.0-beta.25",
    "next-themes": "^0.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss-animated": "^1.1.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.0.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "20.1.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.0.0",
    "tailwindcss": "^3.4.15",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "5.0.4"
  }
}
